{
    "name": "HumHub",
    "id": "humhub",
    "packaging_format": 1,
    "description": {
        "en": "Enterprise Social Network.",
        "fr": "RÃ©seau Social d'Entreprise."
    },
    "url": "https://www.humhub.org",
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "Nils Van Zuijlen",
        "url": "https://github.com/nils-van-zuijlen"
    },
    "version": "1.8.2~ynh2",
    "requirements": {
        "yunohost": ">= 4.1.7"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
				"type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
				"type": "path",
                "example": "/humhub",
                "default": "/humhub"
            },
			{
                "name": "admin",
                "type": "user",
                "example": "johndoe"
            },
			{
                "name": "is_public",
                "type": "boolean",
                "default": false
			}
        ]
    }
}
